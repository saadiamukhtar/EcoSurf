{
    "name": "EcoSurf",
    "description": "Tracks your carbon emissions while surfing",
    "version":"0.1.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["javascript/background.js"]
    },
    "browser_action": {
        "default_icon":  "icon64.png",
        "default_popup": "popup.html",
        "default_title": "EcoSurf"
    },

    
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["javascript/contentscript.js"]
    }]
    
    
    
    
}
